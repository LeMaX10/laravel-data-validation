{
    "name": "lemax10/laravel-data-validation",
    "type": "library",
    "description": "Laravel Data Validation (INN, SNILS, BIK)",
    "keywords": ["inn", "snils", "bik", "data-validation", "php"],
    "homepage": "https://github.com/LeMaX10/laravel-data-validation",
    "license": "MIT",
    "authors": [
        {
            "name": "Laravel Data Validation contributors",
            "homepage": "https://github.com/LeMaX10/laravel-data-validation/graphs/contributors"
        }
    ],
    "autoload": {
